    "version": "0.2.0",
    "configurations": [
        {
            "name": "app.py(ilan)",
            "type": "debugpy",
            "request": "launch",
            "module": "pybricksdev",
            "args": [
                "run",
                "ble",
                "--name",
                "Ilan Prime",
                "app.py"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONUTF8": "1"
            }
        }, 
